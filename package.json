    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-storybook": "^0.6.10",
    "esm": "^3.2.25",
    "fake-indexeddb": "^3.1.8",
    "get-port": "^5.1.1",
